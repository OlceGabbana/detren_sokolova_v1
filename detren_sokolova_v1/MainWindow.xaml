<Window x:Class="detren_sokolova_v1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:detren_sokolova_v1"
        mc:Ignorable="d"
        Title="Кафе" Height="500" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="89"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="30" Foreground="White">Кафе</TextBlock>
        <Grid Background="DarkRed" Panel.ZIndex="-2"/>
        <Grid Grid.Row="2" Background="IndianRed"/>
        <Frame Grid.Row="1" Name="MainFrame"/>
    </Grid>
</Window>
